  m_Script: {fileID: 11500000, guid: 98b7caa80e48e35469bbc71ac397b8f1, type: 3}
  m_Name: TimmyDialogue
  m_EditorClassIdentifier: 
  dialogueLines:
  - line: 'Timmy, how have you been?

'
    lineTime: 5
    voiceOver: {fileID: 0}
    textAudio: {fileID: 8300000, guid: 56bcbc5f8de41a943a6ac8425f0fa664, type: 3}
    characterName: Furgust
    characterSprite: {fileID: 21300000, guid: d87d15b866dfab341b256a900fde80e3, type: 3}
  - line: "Let\u2019s move, quick.\r\n"
    lineTime: 4
    voiceOver: {fileID: 0}
    textAudio: {fileID: 8300000, guid: 56bcbc5f8de41a943a6ac8425f0fa664, type: 3}
    characterName: Timmy
    characterSprite: {fileID: 21300000, guid: 858d6c41e6c19a240ba4439d985496e5, type: 3}
  - line: 'OK, sorry man, where to?

'
    lineTime: 5
    voiceOver: {fileID: 0}
    textAudio: {fileID: 8300000, guid: 56bcbc5f8de41a943a6ac8425f0fa664, type: 3}
    characterName: Furgust
    characterSprite: {fileID: 21300000, guid: d87d15b866dfab341b256a900fde80e3, type: 3}
  - line: 'The MEGA-Mall

'
    lineTime: 4
    voiceOver: {fileID: 0}
    textAudio: {fileID: 8300000, guid: 56bcbc5f8de41a943a6ac8425f0fa664, type: 3}
    characterName: Timmy
    characterSprite: {fileID: 21300000, guid: 858d6c41e6c19a240ba4439d985496e5, type: 3}
  - line: OK, hey are you alright Tim?
    lineTime: 5
    voiceOver: {fileID: 0}
    textAudio: {fileID: 8300000, guid: 56bcbc5f8de41a943a6ac8425f0fa664, type: 3}
    characterName: Furgust
    characterSprite: {fileID: 21300000, guid: d87d15b866dfab341b256a900fde80e3, type: 3}
  - line: "Well\u2026 ugh not really. Look, I know I haven't seen you in a long time,
      but we came up together, I always looked up to you. You watched out for me
      when my family died in that depressurization accident.\r\n"
    lineTime: 9
    voiceOver: {fileID: 0}
    textAudio: {fileID: 8300000, guid: 56bcbc5f8de41a943a6ac8425f0fa664, type: 3}
    characterName: Timmy
    characterSprite: {fileID: 21300000, guid: 858d6c41e6c19a240ba4439d985496e5, type: 3}
  - line: 'Ya of course, you''re like a brother to me Timmy.

'
    lineTime: 5
    voiceOver: {fileID: 8300000, guid: 56bcbc5f8de41a943a6ac8425f0fa664, type: 3}
    textAudio: {fileID: 8300000, guid: 56bcbc5f8de41a943a6ac8425f0fa664, type: 3}
    characterName: Furgust
    characterSprite: {fileID: 21300000, guid: d87d15b866dfab341b256a900fde80e3, type: 3}
  - line: 'Well I need your help again. I owe money to some bad people.

'
    lineTime: 4
    voiceOver: {fileID: 0}
    textAudio: {fileID: 8300000, guid: 56bcbc5f8de41a943a6ac8425f0fa664, type: 3}
    characterName: Timmy
    characterSprite: {fileID: 21300000, guid: 858d6c41e6c19a240ba4439d985496e5, type: 3}
  - line: "Geez Timmy\u2026 who?\r\n"
    lineTime: 4
    voiceOver: {fileID: 0}
    textAudio: {fileID: 8300000, guid: 56bcbc5f8de41a943a6ac8425f0fa664, type: 3}
    characterName: Furgust
    characterSprite: {fileID: 21300000, guid: d87d15b866dfab341b256a900fde80e3, type: 3}
  - line: 'Adrian Atkins, the Taxi tycoon.

'
    lineTime: 4
    voiceOver: {fileID: 0}
    textAudio: {fileID: 8300000, guid: 56bcbc5f8de41a943a6ac8425f0fa664, type: 3}
    characterName: Timmy
    characterSprite: {fileID: 21300000, guid: 858d6c41e6c19a240ba4439d985496e5, type: 3}
  - line: 'What? That guy is bad news, how did you get mixed up with him?

'
    lineTime: 8
    voiceOver: {fileID: 0}
    textAudio: {fileID: 8300000, guid: 56bcbc5f8de41a943a6ac8425f0fa664, type: 3}
    characterName: Furgust
    characterSprite: {fileID: 21300000, guid: d87d15b866dfab341b256a900fde80e3, type: 3}
  - line: 'I was in the wrong place, wrong time.

'
    lineTime: 5
    voiceOver: {fileID: 0}
    textAudio: {fileID: 8300000, guid: 56bcbc5f8de41a943a6ac8425f0fa664, type: 3}
    characterName: Timmy
    characterSprite: {fileID: 21300000, guid: 858d6c41e6c19a240ba4439d985496e5, type: 3}
  - line: "How much do you owe?\r"
    lineTime: 4
    voiceOver: {fileID: 0}
    textAudio: {fileID: 8300000, guid: 56bcbc5f8de41a943a6ac8425f0fa664, type: 3}
    characterName: Furgust
    characterSprite: {fileID: 21300000, guid: d87d15b866dfab341b256a900fde80e3, type: 3}
  - line: "Well, I need 4 million credits in 4 days.\r"
    lineTime: 5
    voiceOver: {fileID: 0}
    textAudio: {fileID: 8300000, guid: 56bcbc5f8de41a943a6ac8425f0fa664, type: 3}
    characterName: Timmy
    characterSprite: {fileID: 21300000, guid: 858d6c41e6c19a240ba4439d985496e5, type: 3}
  - line: "WHAT! Sorry Timmy, I don\u2019t have that kind of money. I would not be
      driving a cab if I did.\r\n"
    lineTime: 6
    voiceOver: {fileID: 0}
    textAudio: {fileID: 8300000, guid: 56bcbc5f8de41a943a6ac8425f0fa664, type: 3}
    characterName: Furgust
    characterSprite: {fileID: 21300000, guid: d87d15b866dfab341b256a900fde80e3, type: 3}
  - line: "I know Furgus, that's where you come in. You have a cab license! I have
      a \u201Cfriend\u201D and he needs a legit courier for some deliveries. He pays
      really well. I can hook you up.\r\n"
    lineTime: 6
    voiceOver: {fileID: 0}
    textAudio: {fileID: 8300000, guid: 56bcbc5f8de41a943a6ac8425f0fa664, type: 3}
    characterName: Timmy
    characterSprite: {fileID: 21300000, guid: 858d6c41e6c19a240ba4439d985496e5, type: 3}
  - line: "I don\u2019t know Timmy, who is this guy? Is it legal?\r\n"
    lineTime: 8
    voiceOver: {fileID: 0}
    textAudio: {fileID: 8300000, guid: 56bcbc5f8de41a943a6ac8425f0fa664, type: 3}
    characterName: Furgust
    characterSprite: {fileID: 21300000, guid: d87d15b866dfab341b256a900fde80e3, type: 3}
  - line: " Don\u2019t worry, it's all above board. He\u2019s an upstanding citizen,
      a real entrepreneur. His name is Raj. I\u2019ll send you his client info when
      you drop me off.\r"
    lineTime: 7
    voiceOver: {fileID: 0}
    textAudio: {fileID: 8300000, guid: 56bcbc5f8de41a943a6ac8425f0fa664, type: 3}
    characterName: Timmy
    characterSprite: {fileID: 21300000, guid: 858d6c41e6c19a240ba4439d985496e5, type: 3}
  - line: "OK, for you Timmy, as long as there is no shady business going on, I\u2019m
      in, I\u2019ll help.\r\n"
    lineTime: 6
    voiceOver: {fileID: 0}
    textAudio: {fileID: 8300000, guid: 56bcbc5f8de41a943a6ac8425f0fa664, type: 3}
    characterName: Furgust
    characterSprite: {fileID: 21300000, guid: d87d15b866dfab341b256a900fde80e3, type: 3}
  - line: 'Me?! Never! You know me, I am as clean as they come! Thanks Furgus!

'
    lineTime: 8
    voiceOver: {fileID: 0}
    textAudio: {fileID: 8300000, guid: 56bcbc5f8de41a943a6ac8425f0fa664, type: 3}
    characterName: Timmy
    characterSprite: {fileID: 21300000, guid: 858d6c41e6c19a240ba4439d985496e5, type: 3}
  dropOffLines:
  - line: "Thanks again Furgust, I need to get inside, I\u2019ve sent you Rajs client
      info. I\u2019ll contact you later to see how it's going.\r\n"
    lineTime: 6
    voiceOver: {fileID: 0}
    textAudio: {fileID: 8300000, guid: 56bcbc5f8de41a943a6ac8425f0fa664, type: 3}
    characterName: Timmy
    characterSprite: {fileID: 21300000, guid: 858d6c41e6c19a240ba4439d985496e5, type: 3}
